{
  "contentTypes": {
    "text": {
      "name": "Texto",
      "description": "Allows sending plain text"
    },
    "carousel": {
      "name": "Carrossel",
      "description": "Allows sending multimedia menu"
    },
    "chatState": {
      "name": "Chat State",
      "description": "Allows sending and receiving information about the conversation's current status"
    },
    "collection": {
      "name": "Collection",
      "description": "Allows sending multiple contents of the same type in a single message"
    },
    "input": {
      "name": "Input",
      "description": "Allows sending of structured information requests to the user, where it is possible to define validations rules"
    },
    "list": {
      "name": "Lista",
      "description": "Allows sending of a list of documents on a single message"
    },
    "location": {
      "name": "Envio de localização",
      "description": "Allows sending and receiving of geographic information"
    },
    "requestLocation": {
      "name": "Pedir localização",
      "description": "Allows ask to the user to send their geographic information"
    },
    "audio": {
      "name": "Áudio",
      "description": "Allows sending audio files"
    },
    "documents": {
      "name": "Documentos",
      "description": "Allows sending document files"
    },
    "gif": {
      "name": "Gif",
      "description": "Allows sending gif files"
    },
    "image": {
      "name": "Imagem",
      "description": "Allows sending image files"
    },
    "video": {
      "name": "Vídeo",
      "description": "Allows sending video files"
    },
    "menu": {
      "name": "Menu",
      "description": "Description"
    },
    "quickReply": {
      "name": "Quick Reply",
      "description": "Description"
    },
    "native": {
      "name": "Native",
      "description": "Allows sending of a native content of some channel using JSON format"
    },
    "paymentInvoice": {
      "name": "Fatura",
      "description": "Allows sending of a payment request to a payment channel"
    },
    "paymentReceipt": {
      "name": "Recibo",
      "description": "Allows sending of a payment receipt to a customer"
    },
    "resource": {
      "name": "Resource",
      "description": "Allows sending of a message where the content is a resource stored in the server"
    },
    "sensitive": {
      "name": "Sensitive",
      "description": "Wraps a message content in order to signal that the information is confidential or sensitive"
    },
    "weblink": {
      "name": "Web Link",
      "description": "Allows sending of a link for a webpage to the client including metadata, such as link's title, description and a miniature image"
    }
  },
  "contentCapabilityPerChannel": {
    "blipChat": {
      "name": "BLiP Chat",
      "svg": "blipChat",
      "support": {
        "text": {
          "isSupported": true,
          "modal": {
            "title": "Texto - BLiP Chat",
            "description": "O texto simples é a forma mais prática e comum para se criar uma experiência conversacional.",
            "img": {
              "src": "textBlipChat",
              "title": "Tipo de conteúdo texto do BLiP Chat",
              "alt": "Tipo de conteúdo texto"
            },
            "docsUrl": "https://docs.blip.ai/#plain-text"
          }
        },
        "carousel": {
          "isSupported": true,
          "modal": {
            "title": "Carrossel - BLiP Chat",
            "description": "Envia uma lista horizontal e rolável de conteúdos, parecido com um menu, porém com suporte a imagens como título. ",
            "img": {
              "src": "carouselBlipChat",
              "title": "Tipo de conteúdo Carrossel do BLiP Chat",
              "alt": "Tipo de conteúdo Carrossel"
            },
            "docsUrl": "https://docs.blip.ai/#carousel"
          }
        },
        "chatState": {
          "isSupported": true,
          "modal": {
            "title": "Digitando (Chat State) - BLiP Chat",
            "description": "Mostra ao usuário que uma mensagem está sendo digitada, como acontece em conversas comuns nos aplicativos de mensageria. ",
            "img": {
              "src": "chatStateBlipChat",
              "title": "Conteúdo tipo digitando do BLiP Chat",
              "alt": "Conteúdo tipo digitando"
            },
            "docsUrl": "https://docs.blip.ai/#chat-state"
          }
        },
        "collection": {
          "isSupported": true,
          "modal": {
            "title": "Collection - BLiP Chat",
            "description": "Envia vários conteúdos do mesmo tipo em uma única mensagem. Alguns canais suportam esse tipo de agregação com layouts especiais.",
            "img": {
              "src": "",
              "title": "Conteúdo tipo Collection do BLiP Chat",
              "alt": "Conteúdo tipo Collection"
            },
            "docsUrl": "https://docs.blip.ai/#collection"
          }
        },
        "input": {
          "isSupported": true,
          "modal": {
            "title": "Input - BLiP Chat",
            "description": "Envia solicitações de informações estruturadas para o usuário, onde é possível definir regras de validação. Isso é útil para criar formulários de perguntas e obter informações específicas do usuário - como nome ou número de telefone - ou informações digitadas - como uma imagem ou local.",
            "img": {
              "src": "",
              "title": "Conteúdo tipo Input do BLiP Chat",
              "alt": "Conteúdo tipo Input"
            },
            "docsUrl": "https://docs.blip.ai/#input"
          }
        },
        "list": {
          "isSupported": false
        },
        "location": {
          "isSupported": true,
          "modal": {
            "title": "Enviar localização - BLiP Chat",
            "description": "Envia uma mensagem com um mapa que aponta um determinada localização geográfica para o usuário.",
            "img": {
              "src": "sendLocationBlipChat",
              "title": "Conteúdo tipo localização do BLiP Chat",
              "alt": "Conteúdo tipo localização"
            },
            "docsUrl": "https://docs.blip.ai/#send-location"
          }
        },
        "requestLocation": {
